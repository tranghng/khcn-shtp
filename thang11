    <script>
        function downloadImage() {
            const captureArea = document.getElementById('capture-area');
            const btn = event.currentTarget;
            const originalText = btn.innerHTML;
            
            btn.innerHTML = '<i class="fas fa-spinner fa-spin mr-2"></i>Đang xử lý...';
            btn.disabled = true;

            html2canvas(captureArea, {
                scale: 2,
                useCORS: true,
                backgroundColor: '#ffffff',
                logging: false,
                onclone: (clonedDoc) => {
                    clonedDoc.getElementById('capture-area').style.display = 'block';
                }
            }).then(canvas => {
                const link = document.createElement('a');
                link.download = 'Infographic_KCNC_Thang11_Final_V13.png';
                link.href = canvas.toDataURL('image/png');
                link.click();
                
                btn.innerHTML = originalText;
                btn.disabled = false;
            }).catch(err => {
                console.error("Lỗi khi tạo ảnh:", err);
                alert("Không thể tạo ảnh tự động. Vui lòng sử dụng nút 'Tải PDF Ngay' bên cạnh để lưu lại báo cáo.");
                btn.innerHTML = originalText;
                btn.disabled = false;
            });
        }

        function downloadDirectPDF() {
            const element = document.getElementById('capture-area');
            const btn = event.currentTarget;
            const originalText = btn.innerHTML;
            
            btn.innerHTML = '<i class="fas fa-spinner fa-spin mr-2"></i>Đang xử lý...';
            btn.disabled = true;

            const opt = {
                margin:       0,
                filename:     'Infographic_KCNC_Thang11_Final.pdf',
                image:        { type: 'jpeg', quality: 0.98 },
                html2canvas:  { scale: 2, useCORS: true, logging: false },
                jsPDF:        { unit: 'in', format: 'a4', orientation: 'portrait' },
                pagebreak:    { mode: 'avoid-all' } 
            };
            
            // Tự động tính toán kích thước thật để in PDF dài (nếu cần)
            // Hoặc ép về khổ A4
            const width = element.offsetWidth;
            const height = element.offsetHeight;
            const widthIn = width / 96;
            const heightIn = height / 96;

            // Sử dụng khổ giấy tùy biến theo nội dung để không bị cắt
            opt.jsPDF.format = [widthIn, heightIn];

            html2pdf().set(opt).from(element).save().then(() => {
                btn.innerHTML = originalText;
                btn.disabled = false;
            }).catch(err => {
                console.error(err);
                alert("Lỗi khi tạo PDF. Vui lòng thử lại hoặc dùng trình duyệt khác.");
                btn.innerHTML = originalText;
                btn.disabled = false;
            });
        }
    </script>

</body>
</html>
